{
    "name": "wireui/wireui",
    "description": "Livewire ui components with tailwind base style",
    "type": "library",
    "keywords": [
        "livewire components",
        "livewire-ui",
        "wireui",
        "laravel components",
        "blade components"
    ],
    "require": {
        "php": "^8.0",
        "symfony/http-kernel": "^5.0|^6.0",
        "illuminate/support": "^8|^9",
        "illuminate/database": "^8|^9",
        "illuminate/validation": "^8|^9",
        "laravel/framework": "^8|^9",
        "livewire/livewire": "^2.9"
    },
    "require-dev": {
        "mockery/mockery": "^1.3.1",
        "phpunit/phpunit": "^9.5",
        "orchestra/testbench": "^6.0|^7.0",
        "orchestra/testbench-dusk": "^6.0|^7.0",
        "psy/psysh": "@stable",
        "friendsofphp/php-cs-fixer": "^3.5"
    },
    "authors": [
        {
            "name": "Pedro Henrique",
            "email": "pedrolivertwd@gmail.com"
        }
    ],
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "WireUi\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\": "vendor/orchestra/testbench-core/laravel/app",
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "./vendor/bin/dusk-updater detect --no-interaction"
        ],
        "test": "./vendor/bin/phpunit --configuration phpunit.xml",
        "phpcs": "./vendor/bin/php-cs-fixer fix"
    },
    "extra": {
        "laravel": {
            "providers": [
                "WireUi\\Providers\\WireUiServiceProvider"
            ],
            "aliases": []
        }
    }
}
